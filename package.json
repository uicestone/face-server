{
  "name": "face-server",
  "description": "社区内控后端",
  "version": "0.9.0",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -T src/server",
    "clean": "rm -rf dist",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
    "seed": "ts-node prisma/seed",
    "migrate": "prisma2 migrate save --experimental",
    "migrate:up": "prisma2 migrate up --experimental",
    "build": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^2.0.0-preview022",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/uuid": "^3.4.7",
    "@types/xlsx": "0.0.36",
    "axios": "^0.21.1",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "14.6.0",
    "graphql-shield": "5.7.3",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "nexus": "^0.12.0-rc.12",
    "nexus-prisma": "^0.10.0",
    "serve-static": "^1.14.1",
    "tencentcloud-sdk-nodejs": "^3.0.118",
    "ts-node": "8.5.2",
    "uuid": "^3.4.0",
    "xlsx": "^0.15.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/jsonwebtoken": "8.3.5",
    "@types/node": "^12.12.7",
    "@types/ws": "6.0.3",
    "prisma2": "^2.0.0-preview022",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.7.2"
  },
  "prettier": {
    "printWidth": 200,
    "semi": false
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
